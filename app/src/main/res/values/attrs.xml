<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="MainPageToolbarBehavior">
        <attr name="finalYPosition" format="dimension" />
        <attr name="startXPosition" format="dimension" />
        <attr name="startToolbarPosition" format="dimension" />
        <attr name="startHeight" format="dimension" />
        <attr name="finalHeight" format="dimension" />
    </declare-styleable>

    <declare-styleable name="ButtonBarContainerTheme">
        <attr name="metaButtonBarStyle" format="reference" />
        <attr name="metaButtonBarButtonStyle" format="reference" />
    </declare-styleable>

    <!--TextBox-->
    <declare-styleable name="EditTextPicker">
        <attr name="required" format="boolean" />

        <attr name="pattern" format="string" />

        <attr name="mask" format="string" />

        <!--Numeric-->
        <attr name="defaultValue" format="string" />
        <attr name="minValue" format="float" />
        <attr name="maxValue" format="float" />
        <attr name="type" format="enum">
            <enum name="range" value="1" />
            <enum name="equal" value="2" />
        </attr>

    </declare-styleable>


    <!--Expandable TextView-->
    <declare-styleable name="ExpandableTextView">
        <attr name="animation_duration" format="integer"/>
        <attr name="collapsed_lines" format="integer"/>
    </declare-styleable>

</resources>